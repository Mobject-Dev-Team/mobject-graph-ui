{
  "name": "mobject-graph-ui",
  "version": "0.1.53",
  "description": "A UI library for graph-based interaction based on mobject-litegraph and mobject-graph",
  "type": "module",
  "main": "dist/js/mobject-graph-ui.bundle.cjs.js",
  "module": "dist/js/mobject-graph-ui.bundle.esm.js",
  "browser": "dist/js/mobject-graph-ui.bundle.js",
  "exports": {
    ".": {
      "import": "./dist/js/mobject-graph-ui.bundle.esm.min.js",
      "require": "./dist/js/mobject-graph-ui.bundle.cjs.min.js",
      "default": "./dist/js/mobject-graph-ui.bundle.min.js"
    },
    "./full": {
      "import": "./dist/js/mobject-graph-ui.bundle.esm.js",
      "require": "./dist/js/mobject-graph-ui.bundle.cjs.js",
      "default": "./dist/js/mobject-graph-ui.bundle.js"
    }
  },
  "scripts": {
    "build": "rollup -c",
    "dev": "npm link && nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mobject-Dev-Team/mobject-graph-ui"
  },
  "keywords": [
    "mobject-graph",
    "mobject"
  ],
  "author": "benhar-dev",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^6.6.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "bootstrap": "^5.3.3",
    "nodemon": "^3.1.9",
    "postcss": "^8.4.49",
    "postcss-discard-duplicates": "^7.0.1",
    "postcss-prefix-selector": "^2.1.0",
    "rollup": "^2.79.2",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-license": "^3.5.3"
  },
  "peerDependencies": {
    "mobject-litegraph": "^0.1.14"
  },
  "dependencies": {
    "stef": "^0.0.1"
  }
}
